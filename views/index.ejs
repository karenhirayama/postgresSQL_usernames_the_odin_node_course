<!DOCTYPE html>
<html>
    <head>
        <title>Users</title>
        <link rel="stylesheet" href="/styles.css">
    </head>
    <body>
        <div class="container">
            <h1>Users</h1>
            
            <% if (users && users.length > 0) { %>
                <div class="users-list">
                    <% users.forEach((user) => { %>
                        <div class="user-item">
                            <span class="message-user"><%= user.username %></span>
                        </div>
                    <% }) %>
                </div>
            <% } else { %>
                <p class="no-users">No users found</p>
            <% } %>
            
            <div class="actions">
                <a href="/create" class="btn">Add new username</a>
            </div>
        </div>
    </body>
</html>